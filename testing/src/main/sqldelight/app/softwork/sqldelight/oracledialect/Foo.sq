CREATE TABLE IF NOT EXISTS foo(
  id INT NOT NULL,
  name VARCHAR(255) NOT NULL,
  name2 VARCHAR2(255 CHAR) DEFAULT NULL,
  nu NUMBER(10, 2) NOT NULL,
  dd DATE NOT NULL,
  tz TIMESTAMP WITH TIME ZONE
);

CREATE USER "A-B" IDENTIFIED BY myStrongPassword;
GRANT CREATE SESSION, CREATE TABLE, CREATE VIEW, CREATE PROCEDURE, CREATE SEQUENCE TO "A-B";
ALTER USER "A-B" QUOTA UNLIMITED ON USERS;

GRANT SELECT, INSERT ON foo TO "A-B";
GRANT SELECT, INSERT, UPDATE, DELETE ON bonuses TO "A-B";
GRANT SELECT ON employees TO "A-B" WITH GRANT OPTION;

getAll:
SELECT * FROM SYSTEM.foo WHERE dd < SYSDATE - 7;

new:
INSERT INTO SYSTEM.foo VALUES ?;

create:
INSERT INTO SYSTEM.foo (id, name, nu, dd, tz) VALUES ?;

testRound:
SELECT round(42), round(42, 0), id FROM SYSTEM.foo;

testDates:
SELECT
  CURRENT_DATE AS "currentDate",
  CURRENT_TIMESTAMP AS "currentTimestamp",
  SYSDATE AS "sysDate",
  SYSTIMESTAMP AS "sysTimestamp",
  LOCALTIMESTAMP AS "localTimestamp",
  id
FROM SYSTEM.foo;

testMinusDate:
SELECT SYSDATE - ?, SYSDATE + coalesce(?, 42), id FROM SYSTEM.foo;
