CREATE TABLE IF NOT EXISTS foo(
  id INT NOT NULL,
  name VARCHAR(255) NOT NULL,
  name2 VARCHAR2(255 CHAR) DEFAULT NULL,
  nu NUMBER(10, 2) NOT NULL,
  dd DATE NOT NULL,
  tz TIMESTAMP WITH TIME ZONE
);

CREATE USER "A-B" IDENTIFIED BY myStrongPassword;
GRANT CREATE SESSION TO "A-B";
GRANT SELECT, INSERT ON foo TO "A-B";

getAll:
SELECT * FROM foo WHERE dd < SYSDATE - 7;

new:
INSERT INTO foo VALUES ?;

create:
INSERT INTO foo (id, name, nu, dd, tz) VALUES ?;

testRound:
SELECT round(42), round(42, 0), id FROM foo;

testDates:
SELECT
  CURRENT_DATE AS "currentDate",
  CURRENT_TIMESTAMP AS "currentTimestamp",
  SYSDATE AS "sysDate",
  SYSTIMESTAMP AS "sysTimestamp",
  LOCALTIMESTAMP AS "localTimestamp",
  id
FROM foo;

testMinusDate:
SELECT SYSDATE - ?, SYSDATE + coalesce(?, 42), id FROM foo;
